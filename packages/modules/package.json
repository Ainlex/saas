{
  "name": "@contafacil/modules",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*",
      "require": "./src/*"
    }
  },
  "scripts": {
    "dev": "echo 'Modules package ready'",
    "build": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test": "node test-events.js"
  },
  "dependencies": {
    "@contafacil/auth": "workspace:*",
    "@contafacil/database": "workspace:*",
    "@contafacil/shared": "workspace:*",
    "eventemitter3": "^5.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}