{
  "name": "@contafacil/auth",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/auth-config.d.ts",
      "import": "./dist/config/auth-config.js",
      "require": "./dist/config/auth-config.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "scripts": {
    "dev": "tsc --build --watch",
    "build": "tsc --build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@contafacil/database": "workspace:*",
    "@contafacil/shared": "workspace:*",
    "next": "14.0.0",
    "next-auth": "^4.24.11",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  }
}